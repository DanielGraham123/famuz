<head>
  <meta charset="UTF-8" />
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Famuz | <%= title %></title>

  <link rel="stylesheet" href="/css/styles.css" />
  <link rel="stylesheet" href="/css/shapes.css" />

  <link rel="shortcut icon" href="/images/logo.png" type="image/x-icon" />

  <link rel="stylesheet" href="/fas/css/all.css" />

  <script src="https://cdn.tailwindcss.com"></script>

  <!-- froala css file. -->
  <link
    href="https://cdn.jsdelivr.net/npm/froala-editor@latest/css/froala_editor.pkgd.min.css"
    rel="stylesheet"
    type="text/css"
  />

  <!-- <link
    rel="stylesheet"
    href="https://unpkg.com/flowbite@1.4.1/dist/flowbite.min.css"
  /> -->
  <script>
    function openSong(id) {
      window.location.href = "/muz-center/songs/" + id;
      console.log("opening song");
    }

    async function deleteSong(id) {
      console.log("delete command", id);

      await fetch(`/muz-center/songs/${id}`, {
        method: "delete",
      })
        .then((result) => {
          console.log("delete song", result);
          window.location.href = "/muz-center/songs";
        })
        .catch((err) => console.error("error: ", err));
    }

    async function deleteCategory(id) {
      console.log("delete category", id);

      await fetch(`/muz-center/categories/${id}`, {
        method: "delete",
      })
        .then((result) => {
          console.log("delete category", result);
          window.location.href = "/muz-center/categories";
        })
        .catch((err) => console.error("error: ", err));
    }
  </script>
</head>
